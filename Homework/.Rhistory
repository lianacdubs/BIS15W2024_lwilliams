library(tidyverse)
library(janitor)
library(here)
library(naniar)
colleges <- read_csv("data/ca_college_data.csv")
colleges <- clean_names(colleges)
glimpse(colleges)
anyNA(colleges)
colleges %>%
count(city) %>%
arrange(desc(n))
colleges %>%
count(city) %>%
top_n(10,n) %>%
ggplot(aes(y=city, x=n))+
geom_col()
