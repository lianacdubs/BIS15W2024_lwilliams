fisheries <- readr::read_csv(file = "data/FAO_1950to2012_111914.csv")
names(fisheries)
dim(fisheries) #This gives the dimensions
summary(fisheries)
View(fisheries)
fisheries <-  clean_names(fisheries)
class(fisheries$Country) <- "Factor"
